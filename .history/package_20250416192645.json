{
  "name": "mootod",
  "private": true,
  "workspaces": [
    "backend",
    "frontend"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",  // สคริปต์สำหรับรัน Vue.js
    "start-backend": "cd ../backend && node server.js",  // รันเซิร์ฟเวอร์ Node.js (Backend)
    "dev": "concurrently \"npm run serve\" \"npm run start-backend\""  // รันทั้งสองพร้อมกัน
  }
}
